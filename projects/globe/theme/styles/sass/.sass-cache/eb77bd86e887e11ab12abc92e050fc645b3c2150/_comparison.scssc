						display: block;
						margin: 20px auto;
					}

					& fieldset{
						padding: 0px;
						margin: 0px;
						border: none;

						& label{
							width: 245px;
							display: block;
							font: 18px $fontReg;
							font-weight: 300;
							color: #ffffff;
						}

						& input[type='text']{
							@include rounded-corners(3px);
							width: 245px;
							height: 35px;
							border: none;
							background: #dee1e5;
							text-indent: 10px;
							color: #164c95;
							font: 18px $fontReg;
						}
						& .error{
							font: 12px $fontReg;
							color: #920202;
							display: block;
							height: 20px;
							line-height: 20px;
							visibility: hidden;
						}

						&.invalid{
							& input[type='email'], & input[type='password'], & input[type='text']{
								border: solid 1px red;
							}

							& span.error{
								visibility: visible;
							}
						}
					}
				}

				& .terms-conditions{
					width: 245px;
					display: block;
					padding: 20px 0px;
					margin: 0px auto;
					border-top: 1px solid #259acc;	
					font: 12px $fontReg;
					color: #ffffff;
				}
			}
		}

		& .payment-success-view{
			@extend .payment-view;
			display: none;

			& .content{
				@include rounded-corners(5px);
				width: 800px;
				min-height: 440px;
				height: auto;
			}

			& .order-info{

				& .order-header{
					@include clearfix();
					min-height: 90px;
					height: auto !important;
					padding-bottom: 20px;

					& span.thank-you{
						font: 36px $fontBold;
						display: block;
						padding: 20px 20px 0px;
					}

					& label{
						font: 18px $fontBold;
						width: 410px !important;
					}

					& span.address{
						margin-left: 20px;
						width: 235px;
						font: 12px $fontReg;
						float: left;
					}

					& span.card-num, & span.card-expiry{
						width: 150px;
						font: 12px $fontReg;
						float: left;
					}
				}
			}

			.total{
				margin: 10px 0px 10px;
				padding-top: 0px !important;
			}
			label.total.width{
				width: 250px !important;
			}

			& .payment-info{
				& input[type=button]{
					@include btn-empty-white('../images/sprites.png');
					border: none;
					margin: 20px auto;
					display: block;
					font-size: 14px;
					line-height: 20px;
					color: #259acc;
				}

				& p{
					border-bottom: 1px solid #156d93;
				}

				& div.border-top{
					border-top: 1px solid #259acc;	
					border-bottom: none; 
				}

				& .go-to-shop{
					@include btn-empty-white('../images/sprites.png');
					display: block;
					font-size: 14px;
					line-height: 30px;
					color: #259acc;
					text-indent: 30px;
					margin: 20px auto;
				}

				& .help{
					font: 24px $fontReg;
					color: #ffffff;
					display: block;
				}
			}
		}

		& .payment-error-view{
			@extend .payment-success-view;
			display: none;

			&.adjiust-height{
				height: 600px !important;
			}

			& .order-info.height-400{
				min-height: 365px !important;

				& .back{
					@include btn-back('../images/sprites.png');
					text-indent: -999em;
					overflow: hidden;
					display: block;
					margin: 140px auto 20px !important;
					margin-top: 35px;
				}
			}

			& .payment-info.height-400{
				min-height: 365px !important;
			}

			& p{
				padding: 0px 20px;
				font: 18px $fontReg;
			} 
		}
	}

	& .pop-up-login{
		@include rounded-corners(5px);
		position: absolute;
		width: 800px;
		min-height: 590px;
		height: auto;
		background: #e8e8e8;
		left: 50%;
		margin-left: -400px;
		top: 20%;
		z-index: 110;
		display: none;

		& .content{
			@include background-gradient(#e6e7e8, #c7d0dc, #e6e7e8);
			@include rounded-corners(5px 5px 0px 0px);
			width: 800px;
			min-height: 365px;
			height: auto;
			border-bottom: 1px solid #9fa6b0;

			& section{
				width: 740px;
				min-height: 400px;
				height: auto;
				margin: 0 auto;
				overflow: hidden;

				& h2{
					width: 740px !important;
				}

				& .login-form-holder{
					width: 330px;
					height: auto;
					float: left;

					& h3{
						font: 30px $fontLight;
						font-weight: 400;
						margin: 20px 0px;
					}

					& fieldset{
						border: none;
						padding: 0px;

						& input[type='email'], & input[type='password']{
							@include rounded-corners(5px);
							@include search-box-shadow($inset: $inset-yes, $horizontal: 4px, $vertical: 6px, $blur: 3px, $spread: -3px, $color: #006773);
							width: 328px;
							height: 43px;
							background: #00a6b9;
							font: 30px $fontLight;
							color: #ffffff;
							text-indent: 10px;
							border: none;
						}

						::-webkit-input-placeholder { /* WebKit browsers */
						    color:    #ffffff;
						}
						:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
						    color:    #ffffff;
						}
						::-moz-placeholder { /* Mozilla Firefox 19+ */
						    color:    #ffffff;
						}
						:-ms-input-placeholder { /* Internet Explorer 10+ */
						    color:    #ffffff;
						}

						& span.error{
							//display: block;
							height: 20px;
							font: 12px $fontLight;
							color: red;
							visibility: hidden;
						}

						&.invalid{
							& input[type='email'], & input[type='password'], & input[type='text']{
								border: solid 1px red;
							}

							& span.error{
								visibility: visible;
							}
						}
					}

					& input[type='submit']{
						background: url('../images/login-sprite.png');
						background-position: 0px -17px;
						border: none;
						float: right;
						width: 98px;
						height: 39px;
						margin-right: -10px;
					}
				}

				& a.link{
					font: 14px $fontReg;
					color: #1853a3;

					&:hover{
						text-decoration: underline;
					}
				}

				& .social-login{
					width: 325px;
					height: auto;
					float: right;

					& h3{
						font: 30px $fontLight;
						font-weight: 400;
						margin: 20px 0px;
					}

					& a.social-login-icons{
						display: block;
						width: 40px;
						height: 40px;
						background: url('../images/login-sprite.png');
						background-position: -117px -13px;
						float: left;
						text-indent: -999em;
						overflow: hidden;
						margin: 0px;
						padding: 0px;

						&.google{
							background-position: -180px -13px;
							margin: 0px 20px;
						}

						&.yahoo{
							background-position: -241px -13px;
						}
					}

					& p{
						clear: both;
						font: 14px $fontReg;
						display: block;
						padding: 30px 0px;
					}
				}
			}
		}

		& .content-bottom{
			width: 740px;
			height: auto;
			margin: 0 auto;
			overflow: hidden;

			& h3{
				font: 30px $fontLight;
				font-weight: 400;
				margin: 20px 0px 0px;
			}

			& .left{
				width: 330px;
				height: auto;
				float: left;
			}

			& .right{
				width: 325px;
				height: auto;
				float: right;
			}

			& p{
				margin: 0px;
				font: 13px $fontLight;
			}

			& .login-icons{
				width: 105px;
				height: 95px;
				display: block;
				text-align: center;
				font: 14px $fontReg;
				float: left;
				margin: 10px 0px 30px;
				color: #000000;

				& .icon{
					width: 70px;
					height: 60px;
					display: block;
					margin: 0 auto;
					background: url('../images/login-sprite.png');
					background-position: -5px -70px;
					text-indent: -999em;
					overflow: hidden;

					&.autopay{
						background-position: -83px -70px;
					}

					&.paperless{
						background-position: -169px -63px;
					}
				}

				&:hover{
					text-decoration: underline;
				}
			}

			& a.checklist{
				display: block;
				font: 14px $fontReg;
				color: #000000;
				margin: 10px 0px;

				& .check{
					width: 15px;
					height: 15px;
					display: block;
					margin: 0 auto;
					background: url('../images/login-sprite.png');
					background-position: -306px -27px;
					text-indent: -999em;
					overflow: hidden;
					float: left;
					margin-right: 10px;
				}

				&:hover{
					text-decoration: underline;
				}
			}
		}
	} 
}


