@import "_config.scss";
@import "_gradient.scss";
@import "_typography.scss";
@import "_slider.scss";


/* ===============
   BASIC LAYOUT
   =============== */

html, body {
	margin: 0;
	padding: 0;
}

#wrapper {
	position: relative;
}



/* Header */
#header {
	
	height: 69px;
	background: $navbar-color-selected;
	position: fixed;
	width: 100%;
	z-index: 100;

	@include drop-shadow-outset;
}

.logo, nav, .searc-box {
	float: left;
}

.logo {
	width: 168px;
	height: 69px;
	position: relative;

	a {
		@include g-logo('../images/sprites.png');
		display: block;
		text-indent: -999em;
		overflow: hidden;
		margin: 16px 0;
	}
	
}

.menu-border {
	@include background-gradient($navyBlue, $lightNavy, $navyBlue);
	width: 1px;
	height: 69px;
	background-repeat: no-repeat;
	display: inline-block;
	float: right;
	position: absolute;
    right: 0;
    top: 0;
}

.search-box {
	width: 126px;
	height: 27px;
	margin: 21px 0;

	display: inline-block;

	form {
		
		position: relative;

		&#search-box {

			input {

				border: 0;
				float: left;

				&.search-textbox {
					height: 25px;
					width: 91px;
					padding: 0px;
					padding-left: 10px;
					padding-right: 25px;

					-webkit-border-radius: 130px;
					border-radius: 130px;

					@include search-box-shadow; 
				}

				&.search-btn {
					@include icon-056('../images/sprites.png');
					display: block;
					position: absolute;
					right: 0;
					top: 2px;
				}

			}
			
		}
	}
}



/* Left Sidebar */

/* Right Sidebar */

/* Footer */



/* ===============
   NAVIGATION
   =============== */
/* Primary */
nav {
	width: 665px;
	height: 69px;

	&.tablet-nav { display:none; }
	ul {

		margin: 0;
		padding: 0;

		li {


			&.nav-menu {
				width: auto;
				float: left;
				list-style: none;
				height: 69px;
				text-align: center;
				position: relative;
				display: inline;
			}

			a {
				float: left;
				color: $navbar-color;
				text-decoration: none;
				font-family: $fontReg;
				font-size: 14px;
				display: block;
				height: 69px;
				padding: 0 17px;
				vertical-align: top;
				line-height: 69px;

				span {

					display: inline-block;
					vertical-align: middle;

					&.icon-prod {
						@include menu-products-inactive('../images/sprites.png');
					}

					&.icon-shop {
						@include menu-shop-inactive('../images/sprites.png');
					}

					&.icon-broadband {
						@include menu-broadband-inactive('../images/sprites.png');
					}

					&.icon-business {
						@include menu-business-inactive('../images/sprites.png');
					}

					&.icon-help-support {
						@include menu-help-support-inactive('../images/sprites.png');

					}
				}

				&.active, &:hover {
					color: $navbar-color-selected;
				}


			}

			&.active, &:hover {
				background: $navbar-color;

				span {
					&.icon-prod {
						@include menu-products-active('../images/sprites.png');

					}

					&.icon-shop {
						@include menu-shop-active('../images/sprites.png');

					}

					&.icon-broadband {
						@include menu-broadband-active('../images/sprites.png');

					}

					&.icon-business {
						@include menu-business-active('../images/sprites.png');
					}

					&.icon-help-support {
						
						@include menu-help-support-active('../images/sprites.png');
					}
		
				}

				.arrow-down {
					@include icon-049('../images/sprites.png');
					position: absolute;
					bottom: -5px;
					left: 50%;
					margin-left: -3.5px;
					width: 7px;
				}
			}


		}

	}
}

.sub-holder {

	width: 100%;
	background: $navbar-color;
	position: absolute;
	top: 69px;
	display: none;
	z-index: 100;

	.sub-nav-prod {
		display: block;
		width: 960px;
		height: auto;
		margin: 0 auto;
		list-style: none;

		li {
			float: left;
			width: 120px;
			line-height: 1.5;
			text-align: center;
			overflow: hidden;

			a {
				text-align: center;
				text-decoration: none;
				display: block;
				padding: 20px 10px;
				margin: 5px 0;
				width: 100px;
				height: auto;


				span {

					display: inline-block;
					vertical-align: middle;
					margin: 0;
					margin-bottom: 10px;

					&.sub-phones {
						@include sub-phones('../images/sprites.png');
					}

					&.sub-postpaid {
						@include sub-postpaid('../images/sprites.png');
					}

					&.sub-prepaid {
						@include sub-prepaid('../images/sprites.png');
					}

					&.sub-mobile-net {
						@include sub-mobile-net('../images/sprites.png');
					}

					&.sub-international {
						@include sub-international('../images/sprites.png');
					}

					&.sub-download-apps {
						@include sub-download-apps('../images/sprites.png');

					}

					&.sub-gcash {
						@include sub-gchash('../images/sprites.png');

					}

					&.sub-rewards {
						@include sub-rewards('../images/sprites.png');

					}
				}

				&:hover {

					background: $silver;
					@include rounded-corners;

				}

				p {
					margin: 10px 0;
					text-decoration: none;
				}

			}
		}
	}	

}



