/* ===============
   BASIC LAYOUT
   =============== */

html, body {
	margin: 0;
	padding: 0;
}

#wrapper {
	position: relative;
}



/* Header */
#header {
	
	height: 69px;
	background: $navbar-color-selected;
	position: fixed;
	width: 100%;
	z-index: 100;

	@include drop-shadow-outset;
}

.logo, nav, .searc-box {
	float: left;
}

.logo {
	width: 168px;
	height: 69px;
	position: relative;

	a {
		@include g-logo('../images/sprites.png');
		display: block;
		text-indent: -999em;
		overflow: hidden;
		margin: 16px 0;
	}
	
}

.menu-border {
	@include background-gradient($navyBlue, $lightNavy, $navyBlue);
	width: 1px;
	height: 69px;
	background-repeat: no-repeat;
	display: inline-block;
	float: right;
	position: absolute;
    right: 0;
    top: 0;
}

.search-box {
	width: 126px;
	height: 27px;
	margin: 21px 0;

	display: inline-block;

	form {
		
		position: relative;

		&#search-box {

			input {

				border: 0;
				float: left;

				&.search-textbox {
					height: 25px;
					width: 91px;
					padding: 0px;
					padding-left: 10px;
					padding-right: 25px;

					-webkit-border-radius: 130px;
					border-radius: 130px;

					@include search-box-shadow; 
				}

				&.search-btn {
					@include icon-056('../images/sprites.png');
					display: block;
					position: absolute;
					right: 0;
					top: 2px;
				}

			}
			
		}
	}
}



