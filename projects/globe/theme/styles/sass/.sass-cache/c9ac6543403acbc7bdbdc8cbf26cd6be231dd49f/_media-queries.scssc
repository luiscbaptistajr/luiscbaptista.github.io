.mobile {
    display:none;
}

.tablet {
    display:none;
}



/* Tablet Portrait size to standard 960 (devices and browsers) */
@media only screen and (min-width: 768px) and (max-width: 959px) {
    
    #header .main {
        width:100%;
        .logo { margin-left:20px; }
        nav { 
            display:none;
            &.tablet-nav {
                background-color:#005ab0;
                display:block;
                width:70px;
                position:relative;
                cursor:pointer;
                //.menu-border { display:none; }
                ul {
                    position:absolute;
                    top:100%;
                    width:345px;
                    overflow:hidden;
                    display:none;

                    &:target {
                        display:block;
                    }

                    .tablet-sub-nav-prod {
                        background-color:#0f6bac;
                        position:relative;
                        border-top:1px solid #083866;
                        top:0;
                        line-height:12px;
                        width:100%;
                        /* IE 8 */
                        -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=80)";
                        /* IE 5-7 */
                        filter: alpha(opacity=80);
                        /* Netscape */
                        -moz-opacity:0.8;
                        /* Safari 1.x */
                        -khtml-opacity:0.8;
                        /* Good browsers */
                        opacity:0.8;

                        li {
                            background-color:transparent;
                            &:last-child { border-bottom:none; }
                            a { background-color:transparent; }
                            .icon-039 {
                                background: url(../images/sprites.png) 0px -650px;
                                width:28px;
                                height:27px;
                                margin-top:5px;
                                margin-right:10px;
                                float:left;
                            }
                        }
                    }

                    li {
                        border-top:1px solid #327ebc;
                        border-bottom:1px solid #083866;
                        &:first-child { border-top:none; }
                        &:hover { background-color:transparent; }
                        a {
                            background-color:#1953a4;
                            color:white;
                            height:40px;
                            line-height:40px;
                            &:hover {
                                background-color:white;
                                color:#1953a4;
                            }
                        }
                    }

                }

                .nav-menu {
                    float:left;
                    width:100%;
                    height:auto;
                }

                .menu-launcher {
                    float:right;
                    width:40px;
                    margin:10px 5px 10px 0;
                    display:inline-block;

                    span {
                        margin:6px 5px;
                        display:block;
                        border:2px solid white;
                        -webkit-border-radius:5px;
                        -moz-border-radius:5px;
                        border-radius:5px;
                    }
                }

                a {
                    float:none;
                    height:auto;
                    text-align:left;
                    white-space:nowrap;
                    line-height:30px;
                    display:block;

                    > span {
                        display:none;
                    }
                }

            }
        }
        .search-box {
            position:absolute;
            right:20px;
        }
    }
    .slider {
        width:100% !important;
        height:538px !important;

        ul {
            width:100% !important;
            li { width:80% !important; }
        }
        .share-box { width:10%; }
        .title-box {
            width:40%;
            h2 {
                font-size:32px;
                line-height:32px;
            }
        }
        .content-box {
            width:40%;
            p {
                font-size:18px !important;
                line-height:20px !important;
            }
        }
        .hero-box { 
            width:40%;
            img { width:100%; }
            .slide-hero img, .thumbnail-box a img { width:auto; }
            object, embed, iframe {
                width:100%;
                height:50%;
                margin-left:0px;
            }
        }
    }
    .slider-box {
        height:530px;
        overflow:hidden;
    }
    .box-960 {
        width:100%;
        .breadcrumbs-holder {
            .bread-left { 
                margin-left:20px;
                width:35%;
                h2 { font-size:24px; }
            }
            .bread-list {
                width: 60%;
                font-size: 10px;
                .bread-link { font-size:14px; }
            }
        }
        #home-box-002 { padding-left:20px; }
        .home-leftCol-005, .home-rightCol-005 { width:28% !important; }
        .mobile-features { width:100%; }
        .prepaid-promos {
            .featured-title { width:100%; }
            .promo-list-holder {
                padding-left:20px;
                .promo-list {
                    width:48% !important;
                    img { float:none;}
                    .w425 { width:96.5%; }
                }
                .w425 {
                    float:right;
                    img { width:96.5%; }
                }
            }
        }
        #subscribe-news-letter { padding-left:40px; padding-right:40px; }
        .page-title { margin-left:20px; font-size:24px; }
        .cart-quickview { margin-right:60px; font-size:24px; }
        &.mini-4-span-slider > ul { width:500% !important; }
    }
    .main-box {
        .static-holder { width:100%; height:100%; }
    }
    .half-box {
        height:auto;
        .left-box-480, .right-box-480 {
            width:100% !important;
            height:512px !important;
            .broadband-box {
                padding-left: 20px;
                padding-right: 20px;
                width: auto;
                min-height: 300px;

                .broadband-image-480 { margin-right: 20px; float:left; }
                span { position:relative; float:left; width:30%; }
                .white-title, .black-title { font-size:22px; }
                .white-text, .black-text { font-size:12px; }
                .apply-now {}
            }
            .box-480 { height:100%; }
            .mini-2-span-slider > ul { width:500% !important; }
        }
        .left-box-480 { 
            #home-box-003 { padding-left:20px; }
            .mini-2-span-group {
                width:25%;
                .slides { width:40%; }
            }
            .mini-slider-box { height:100% !important; }
        }
        .right-box-480 {
            #home-box-004 { 
                width:70%;
                span { width:50%; }
            }
            .mobile-post-plan {
                padding-right:20px;
                .subscribe { left:0 !important; }
            }
        }
    }

    #footer .main { 
        width:100%;
        ul {
            width:15% !important;
            padding-left:30px !important;
        }
    }

}

/* Mobile Landscape Size to Tablet Portrait (devices and browsers) */
@media only screen and (min-width: 480px) and (max-width: 767px) {}

/* Mobile Portrait Size to Mobile Landscape Size (devices and browsers) */
@media only screen and (max-width: 479px) {
    
    .mobile {
        display:block;
    }

    .desktop {
        display:none !important;
    }

    .tablet {
        display:none !important;
    }

    *:target {
        z-index:10;
    }
    
    .sticky-login {
        right:20px;
    }

    .main {
        @include clearfix();
    }

    .box-960, .main, .main-box {
        width:320px;
        min-height:0;

        #home-box-005 {
            min-height:0;

            .home-leftCol-005, .home-rightCol-005 {
                width:auto;
                padding:20px 10px;   
            }

        }

        // mobile apps
        .mobile-features {
            width:320px;
        }

    }

    .half-box {
        width:320px;
        min-height:0px;

        .left-box-480, .right-box-480 {
            position:relative;
            float:none;
            width:auto;
            min-height:0px;
            padding:20px 10px;
            overflow:hidden;

            img {
                top:0;
                z-index:0;
            }

             #home-box-003, #home-box-004 {
                position:relative;
                z-index:1;
                
                > span {
                    margin:0;
                    display:inline-block;
                }

                a {
                    position:relative;
                }
            }

            .broadband-box {
                padding:0;

                h3 {
                    line-height:1;
                    font-size:26px;
                }

                img {
                    width:40%;
                    height:auto;
                    margin-right:10px;
                }

                > span {
                    width:55%;
                }

                p {
                    margin:8px 0;
                    line-height:1.25;
                }
            }

        }

        span {
            display:inline-block;

            a.apply-now, a.learn-more {
                position:relative;
                top:auto;
                left:auto;
            }
        }

    }

    .slider-box {
        width:100%;
        display:none;

        ul {
            height:400px;
            margin:0;
            width:100%;
        }

        .share-box {
            width:100%;
        }
    }

    #header {

        position:relative;
        height:auto;
        @include clearfix();

        h1 {
            width:auto;
            height:auto;
            margin:0 30px 0 50px;

            a {
                font-size:12px;
                margin:10px 0;
            }
        }

        nav {
            position:auto;
            float:left;
            width:auto;
            height:auto;
            
            ul {
                position:absolute;
                top:57px;
                right:0;
                width:100%;
                overflow:hidden;
                display:none;
                border-top:1px solid #063964;
                border-bottom:1px solid #2e7cba;
                background:#1953a4;

                &:target {
                    display:block;
                }
                
                .arrow-down {
                    display:none;
                }

                li {
                    border-top:1px solid #2e7cba;
                    border-bottom:1px solid #063964;
                    background:none;
                }
            }

            .nav-menu {
                float:left;
                width:100%;
                height:auto;
            }

            .menu-launcher {
                float:right;
                width:40px;
                padding:10px 5px 10px;
                display:inline-block;

                &.active {
                    background:#1953a4;
                }

                span {
                    margin:6px 5px;
                    display:block;
                    border:2px solid white;
                    -webkit-border-radius:5px;
                    -moz-border-radius:5px;
                    border-radius:5px;
                }
            }

            a {
                float:none;
                height:auto;
                text-align:left;
                color:white;
                white-space:nowrap;
                line-height:50px;
                display:block;

                > span {
                    display:none;
                }
            }

        }
        
        .menu-border {
            display:none;
        }

        .search-box {
            float:right;
            width:auto;
            height:auto;
            margin:0;
            line-height:0;
            border-left:1px solid #003970;

            a.search-launcher {
                padding:10px;
                display:inline-block;
                &.active {
                    background:#1953a4;
                }

                span {
                    width:22px;
                    height:22px;
                    margin:7px 0px;
                    display:inline-block;
                    background:transparent url(../images/sprites.png) no-repeat -150px -26px
                }
            }

            form {
                position:absolute;
                top:100%;
                right:0;
                display:none;

                &:target {
                    display:block;
                }
            }

            form#search-box input.search-textbox {
                width:auto;
                -webkit-border-radius: 0px;
                -moz-border-radius: 0px;
                border-radius: 0px;
            }
        }

        .products-menu-launcher {
            position:absolute;
            top:0;
            left:0;
            border-right:1px solid #003970;

            span {
                margin:20px 9px 3px;
                border-style:solid;
                border-color:white transparent transparent transparent;
                border-width:15px 9px;
                display:inline-block;
            }

            &.active {
                background:#1953a4;
                z-index:11;
            }

        }

        .other-menu {
            position:absolute;
            top:56px;
            left:0;
            width:100%;
            background:#1953a4;
            border-top:1px solid #003970;
            display:none;

            p {
                margin:0 10px;
                line-height:55px;
                color:white;
                font-family:$fontReg;
            }

            ul {
                margin:0;
                border-top:1px solid #063964;
                border-bottom:1px solid #2e7cba;
            }

            li {

                border-top:1px solid #2e7cba;
                border-bottom:1px solid #063964;

                a {
                    width:100%;
                    color:white;
                    line-height:50px;
                    font-size:14px;
                    font-family:$fontReg;
                    display:inline-block;

                    span {
                        width:15px;
                        height:15px;
                        margin:0 5px 0 10px;
                        display:inline-block;
                        vertical-align:middle;
                        background:url(../images/sprites.png) no-repeat -234px -15px;
                    }
                }

            }
            
            &:target {
                display:block;
            }

        }
    }

    #content {
        padding:0;
    }

    #footer {
        height:auto;
        position:relative;
        overflow:hidden;

        ul {
            width:50%;
            padding:15px 0;

            li {
                margin:0 0 0 15px;
            }
        }
    }


    // home 
    .box-960 #home-box-002 {

        background-position:-410px -10px;
        padding:20px 10px;

        > span {
            width:auto;
        }

    }

    .box-960 #home-box-005 {
        background-position:95px bottom;
    }

    // gcash
    .box-960 .breadcrumbs-holder {
        height:auto;
        padding:10px;
        line-height:1;
        overflow:hidden;

        .bread-left {

            position:relative;
            top:50%;

            h2 {
                margin:10px;
                vertical-align:middle;
                display:inline-block;
            }

            span {
                position:relative;
                top:auto;
                margin:0;
                display:none !important;
                vertical-align:middle;
            }

        }

        .bread-list {
            clear:left;
            float:left;
            a {
                margin:10px 0;
                display:block;
            }
        }
    }
    

    // mobile apps
    .box-960 .mobile-features {

        padding:20px 0 0;

        .featured-list-holder,
        .unlimited-list-holder,
        .videos-list-holder,
        .post-rewards-list-holder {

            width:100%;
            line-height:0;
            font-size:0;
            text-align:center;

            li, li.featured-list, li.unlimited-list, li.videos-list, li.post-rewards-list {
                float:none;
                width:49%;
                min-height:230px;
                margin:0;
                line-height:1;
                text-align:center;
                vertical-align:top;
                display:inline-block;

                .featured-image, .unlimited-image, .videos-image {
                    padding-bottom:5px;
                    display:inline-block;
                }
            }

            li.videos-list {
                width:100%;
                min-height:0;
                padding-bottom:20px;
            }

            li.unlimited-list {
                min-height:0;
            }

            a {
                display:block;
            }

            span {
                display:block;
            }

        }
    }

    .box-960 #subscribe-news-letter {
        padding:20px 10px;
        overflow:hidden;

        .subscription-copy {
            width:70%;
        }

        #subscriber-email-box {

            .email-textbox {
                width:100%;
                margin:0 -5px 10px;
                padding:0 5px;
            }
        }
    }

    // postpaid
    .box-960 .prepaid-promos h3 {
        padding:0;
    }

    .main-box {

        .static-holder {
            display:none;
        }
    }


    // prepaid
    .box-960 .prepaid-promos {
        padding:20px 0 0;

        .promo-list-holder {
            width:100%;

            li.promo-list {
                width:auto;
                min-height:0;
                margin:0;
                padding:20px 20px 0;
                
                > span {
                    width:auto;
                }

                p {
                    margin:5px 0 10px;
                }

                h4 {
                    line-height:1;
                }

                img {
                    display:none;
                }
            }
        }
    }

    .floating-nav-holder {
        width:100%;
        top:auto;
        left:0;
        bottom:10px;
        min-height:0;
        overflow:hidden;

        .floating-nav-list li {
            float:left;
            width:20%;
            height:auto;
            padding:0;

            a {
                padding:10px 0;
                display:block;
            }
        }
    }


    // downloads
    .downloads .main-box {
        @include clearfix();
    }

    .downloads {
        .page-title {

            margin:20px;

            span {
                display:none;
            }
        }

        .cart-quickview {
            float:left;
            margin:10px 20px;
        }

        .bg-gray {
            display:none;
        }

        aside {
            display:none;
        }

        .product-list {

            width:100%;
            margin:0;

            .tabs {
                display:none;
            }

            .products li {
                width:auto;
                margin:10px 0 0;
            }
        }
    }

    // bridgecom
    .bridgecom .box-960 {
        width:auto;

        .i-btns {
            width:auto;
            white-space:normal;
            margin:20px 10px;

            a {
                margin:10px 5px 0;
            }
        }

        .desc img {
            margin:10px;
        }

        .map {
            width:100%;
            height:auto;
        }
    }

